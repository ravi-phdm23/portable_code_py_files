r\n  padding: 20px;\r\n  text-align: center;\r\n  color: #7f8c8d;\r\n}\r\n\r\n.loading-message {\r\n  text-align: center;\r\n  padding: 40px;\r\n  color: #7f8c8d;\r\n  font-size: 16px;\r\n}\r\n\r\n/* Statistical Tests Configuration */\r\n.statistical-tests-config {\r\n  margin-top: 15px;\r\n  padding: 15px;\r\n  background: #ffffff;\r\n  border-radius: 6px;\r\n  border: 1px solid #e0e0e0;\r\n}\r\n\r\n.test-types {\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.test-type-label {\r\n  display: block;\r\n  font-weight: 600;\r\n  color: #34495e;\r\n  margin-bottom: 8px;\r\n}\r\n\r\n.checkbox-label-inline {\r\n  display: inline-flex;\r\n  align-items: center;\r\n  margin-right: 20px;\r\n  margin-bottom: 8px;\r\n  cursor: pointer;\r\n}\r\n\r\n.checkbox-label-inline input[type=\"checkbox\"] {\r\n  margin-right: 6px;\r\n  cursor: pointer;\r\n}\r\n\r\n.checkbox-label-inline span {\r\n  font-size: 14px;\r\n  color: #555;\r\n}\r\n\r\n.comparison-strategy {\r\n  margin-top: 15px;\r\n}\r\n\r\n.comparison-strategy label {\r\n  display: block;\r\n  font-weight: 600;\r\n  color: #34495e;\r\n  margin-bottom: 8px;\r\n}\r\n\r\n.strategy-select {\r\n  width: 100%;\r\n  max-width: 350px;\r\n  padding: 8px 12px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n  background: white;\r\n  cursor: pointer;\r\n}\r\n\r\n.strategy-select:disabled {\r\n  background: #f5f5f5;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.strategy-hint {\r\n  margin-top: 8px;\r\n  font-size: 13px;\r\n  color: #7f8c8d;\r\n  font-style: italic;\r\n}\r\n"],"names":[],"ignoreList":[],"sourceRoot":""}